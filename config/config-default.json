{ "Instances":
  { "App.Routing":
    { "engine": "Yolo.Routing"
    , "events": [ [ "Http:responder-add"
                  , { "type": "webpage", "handler": "Yolo.Ui.Builder:http-response" }
                  ]
                ]
    }
  , "App.Routing.Http": { "engine": "Adapter.Http.Server" }
  }

, "Config":
  { "App.Routing.Http": { "port": 7000, "Formidable": {} }
  , "Service.Http.Session": { "lifetime": 86400 }
  , "Yolo.Ui.Runtime": { "cache": 60 }
  , "Language.Jade": { "debug": false, "globals": ["toplevel", "Yolo"] }
  }

, "Logs":
  { "debug": "none"
  , "info": "none"
  , "notice": "syslog"
  , "warning": "syslog"
  , "error": "syslog"
  , "critical": "syslog"
  , "emergency": "syslog"
  }

}
