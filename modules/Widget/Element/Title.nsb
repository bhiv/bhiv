Widget.Element {
  - level: 1

  + Less style:
    .widget-element-title {
      line-height: 2em;
      font-size: 2em;
    }

  > Jade html:
    if data.level > 0
      - var opentag = ['<h', data.level, ' id="', id, '" class="', tags.join(' '), '">'].join('');
      - var closetag = ['</h', data.level, '>'].join(' ');
      !=  opentag + jade.escape(data.text) + closetag
    else
      div(id=id,class=tags.join(' '))= data.text

}
