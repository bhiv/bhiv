Widget.Element {

  - href: null
  - title: null
  - text: null
  - click: null

  + Less style: .widget-element-link { display: inline-block; }

  > Jade html:
    - var href = data.href;
    - if (href == null) href = 'javascript:void 0;';
    a( id=id, class=tags.join(' '), href=data.href, title=data.title
     , x-bind = data.click ? 'click:' + data.click : false
     )
      != invoke(":content")

}